let delim = [' ''\t''\n']
let ws = delim+
let letter = ['A'-'Z''a'-'z']
let digit = ['0'-'9']
let digits = digit+
let id = letter(letter|digit)*


rule tokens = 
    'a'        { return 'A' }
  | 'b'    { return 'B' }
  | 'c'       { return 'C' }               
  | 'd'       { return 'D' }
  | 'g'       { return 'G' }
    | 'h'       { return 'H' }